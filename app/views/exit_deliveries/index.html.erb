<div class="exit-delivery">

  <p id="notice"><%= notice %></p>

  <h1>Exit Deliveries</h1>

  <table>
    <thead>
      <tr>
        <th>Url</th>
        <th>Today Count</th>
        <th>Total Count</th>
        <th>Default</th>
        <th>Percentage</th>
        <th>Status</th>
        <th>Priority</th>
        <th>Cap</th>
        <th>Source</th>
        <!-- <th>Count</th>
        <th>Operational</th> -->
        <th>functional</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @exit_deliveries.each do |exit_delivery| %>
        <tr>
          <td><%= exit_delivery.url %></td>
          <td><%= exit_delivery.count %></td>
          <td><%= exit_delivery.total_count %></td>
          <td><%= exit_delivery.is_default %></td>
          <td><%= exit_delivery.percentage %></td>
          <td><%= exit_delivery.status %></td>
          <td><%= exit_delivery.priority %></td>
          <td><%= exit_delivery.cap %></td>
          <td><%= exit_delivery.source %></td>
          <td><%= exit_delivery.functional %></td>
  <!--         <td><%= exit_delivery.count %></td>
          <td><%= exit_delivery.operational %></td> -->
          <td><%= link_to 'Show', exit_delivery %></td>
          <td><%= link_to 'Edit', edit_exit_delivery_path(exit_delivery) %></td>
          <td><%= link_to 'Destroy', exit_delivery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Exit Delivery', new_exit_delivery_path %>

</div>
